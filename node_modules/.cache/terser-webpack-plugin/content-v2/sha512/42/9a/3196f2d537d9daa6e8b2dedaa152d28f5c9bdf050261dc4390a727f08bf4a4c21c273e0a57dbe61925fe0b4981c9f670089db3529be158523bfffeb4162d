--删除字段\\n--info.sex=nil;\\ninfo[\"sex\"]=nil;\\n\\n--修改\\ninfo[\"name\"]=\"jerry\";\\ninfo.id=789;\\n\\nfor k,v in pairs(info) do\\n    print(k,\"===\",v);\\nend\\n]]')]),n(\"span\",{pre:!0,attrs:{class:\"token comment\"}},[s._v(\"--\")]),s._v(\"\\n\\n\\n\"),n(\"span\",{pre:!0,attrs:{class:\"token comment\"}},[s._v('--[[\\ninfo2={\"abc\",123,true};\\n\\nfor k,v in pairs(info2) do\\n    print(k,\"===\",v);\\nend\\nprint(\"\\\\n\");\\n\\n--增加\\ninfo2[33]=33;\\n\\n--修改\\ninfo2[2]=\"222\";\\n\\n--删除\\ninfo2[1]=nil;\\nfor k,v in pairs(info2) do\\n    print(k,\"===\",v);\\nend\\n\\n]]')]),n(\"span\",{pre:!0,attrs:{class:\"token comment\"}},[s._v(\"--\")]),s._v(\"\\n\\n\\ninfo3\"),n(\"span\",{pre:!0,attrs:{class:\"token operator\"}},[s._v(\"=\")]),n(\"span\",{pre:!0,attrs:{class:\"token punctuation\"}},[s._v(\"{\")]),s._v(\"\\n    name\"),n(\"span\",{pre:!0,attrs:{class:\"token operator\"}},[s._v(\"=\")]),n(\"span\",{pre:!0,attrs:{class:\"token string\"}},[s._v('\"tom\"')]),n(\"span\",{pre:!0,attrs:{class:\"token punctuation\"}},[s._v(\",\")]),s._v(\"\\n    age\"),n(\"span\",{pre:!0,attrs:{class:\"token operator\"}},[s._v(\"=\")]),n(\"span\",{pre:!0,attrs:{class:\"token number\"}},[s._v(\"123\")]),n(\"span\",{pre:!0,attrs:{class:\"token punctuation\"}},[s._v(\",\")]),s._v(\"\\n    sex\"),n(\"span\",{pre:!0,attrs:{class:\"token operator\"}},[s._v(\"=\")]),n(\"span\",{pre:!0,attrs:{class:\"token keyword\"}},[s._v(\"false\")]),n(\"span\",{pre:!0,attrs:{class:\"token punctuation\"}},[s._v(\",\")]),s._v(\"\\n    \"),n(\"span\",{pre:!0,attrs:{class:\"token number\"}},[s._v(\"111\")]),n(\"span\",{pre:!0,attrs:{class:\"token punctuation\"}},[s._v(\",\")]),s._v(\"\\n    \"),n(\"span\",{pre:!0,attrs:{class:\"token string\"}},[s._v('\"222\"')]),n(\"span\",{pre:!0,attrs:{class:\"token punctuation\"}},[s._v(\",\")]),s._v(\"\\n    \"),n(\"span\",{pre:!0,attrs:{class:\"token punctuation\"}},[s._v(\"{\")]),n(\"span\",{pre:!0,attrs:{class:\"token string\"}},[s._v('\"abc\"')]),n(\"span\",{p